<div class="es-pane es-pane--hidden" id="video-pane-match" aria-hidden="true" tabindex="0">
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <h1 class="govuk-heading-l">We have confirmed this is you</h1>
      <p>We have confirmed this is you. You can now continue to check your answers.</p>
      <div class="videoRecorder__videoWrap">
        <video class="videoRecorder__video-preview">Your browser does not support video</video>
        <span class="govuk-tag videoRecorder__tag">Preview</span>
      </div>
      <div class="govuk-button-group">
        <form method="POST" id="video-check-in">
          <input type="hidden" name="_csrf" value="{{ csrfToken }}" />
          <input type="hidden" name="videoMatch" value="MATCH" />
          <button type="submit" class="govuk-button" data-module="govuk-button">Continue</button>
        </form>
      </div>
    </div>
  </div>
</div>
